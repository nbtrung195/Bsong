<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">


<urlrewrite>
	<rule>
		<from>/(.+)/(.+)-([0-9]+)$</from>
		<to>/detail?id=$3</to>
	</rule>
	<rule>
		<from>/(.+)-([0-9]+)/page/([0-9]+)$</from>
		<to>/cat?id=$2&amp;page=$3</to>
	</rule>
	<rule>
		<from>/(.+)-([0-9]+)$</from>
		<to>/cat?id=$2</to>
	</rule>
	<rule>
		<from>/page/([0-9]+)$</from>
		<to>/index?page=$1</to>
	</rule>
	<rule>
		<from>lien-he$</from>
		<to>/contact</to>
	</rule>
</urlrewrite>

